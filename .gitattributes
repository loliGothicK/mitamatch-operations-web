# Enforce LF for TypeScript/JavaScript files
*.ts     text eol=lf
*.tsx    text eol=lf
*.js     text eol=lf
*.jsx    text eol=lf
*.mjs    text eol=lf
*.cjs    text eol=lf
*.json   text eol=lf
*.jsonc  text eol=lf
*.html   text eol=lf
*.css    text eol=lf
*.scss   text eol=lf
*.md     text eol=lf
*.yml    text eol=lf
*.yaml   text eol=lf
*.xml    text eol=lf
*.sh     text eol=lf
*.env    text eol=lf
*.env.*  text eol=lf
*.txt    text eol=lf
.gitattributes text eol=lf
.*rc     text eol=lf
